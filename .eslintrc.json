{
  "extends": ["eslint:recommended"],
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "plugins": [
    "babel"
  ],
  "rules": {
    "no-console": 0,
    "quotes": [2, "double"],
    "semi": [2, "always"],
    "no-trailing-spaces": 2,
    "no-whitespace-before-property": 2,
    "no-multi-spaces": 2,
    "no-multiple-empty-lines": 2,
    "curly": 2,
    "object-curly-spacing": [2, "always"],
    "eol-last": 2,
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "handle-callback-err": 2,
    "no-underscore-dangle": 0,
    "global-require": 0
  }
}